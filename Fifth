//Program to get number using get() functon and display it by using put() function !!!!!!!!!!!! 


#include <iostream>
using namespace std;
class A
{
    public:
    class B
   {
       private:
       int num;
       public:
       void getdata(int n){
          
            num=n;
       }
       void putdata(){
         
           cout<<"\nThe number is="<<num;
       }
   };
};
int main() {

     cout<<"Nested classes in C++";
     A::B obj;
     obj. getdata(9);
     obj. putdata ();
    
    return 0;
}
